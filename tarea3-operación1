import weka.classifiers.Classifier;
import weka.classifiers.trees.J48;
import weka.classifiers.trees.RandomForest;
import weka.classifiers.trees.REPTree;
import weka.classifiers.trees.DecisionStump;
import weka.classifiers.trees.M5P;
import weka.classifiers.trees.SimpleCart;

public class AlgoritmosArboles {
    public static void main(String[] args) {
        // Ejemplo de uso de algoritmos de árboles de decisión en Weka
        Classifier j48 = new J48(); // Árbol de decisión J48
        Classifier randomForest = new RandomForest(); // Random Forest
        Classifier repTree = new REPTree(); // REPTree
        Classifier decisionStump = new DecisionStump(); // DecisionStump
        Classifier m5p = new M5P(); // M5P
        Classifier simpleCart = new SimpleCart(); // SimpleCart
    }
}
